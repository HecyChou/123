v_userconfig = function(   ){

    var vuc = {l:'',c:'',r:''};
    var vucstr = localStorage.getItem('V_UC');

    if( vucstr != null ) vuc = JSON.parse( vucstr ); 

    var channel  = vuc.c;
    var refer = vuc.r;  
    if(channel == null  )
        channel = 'default';
    if(refer == null )
        refer = "default";

    if( v_customer_settings[ refer ] == null )
        return v_customer_settings['default'];
    
    return v_customer_settings[ refer ];
}

